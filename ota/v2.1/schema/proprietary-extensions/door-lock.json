{
  "$id": "https://schemas.ruter.no/adt/ota/api/v2.1/proprietary-extensions/door-lock.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DoorLock",
  "type": "object",
  "required": [
    "eventTimestamp",
    "locked"
  ],
  "description": "This message is used to track if the tram doors are locked or unlocked.",
  "additionalProperties": true,
  "properties": {
    "eventTimestamp": {
      "$id": "#/properties/eventTimestamp",
      "type": "string",
      "description": "ISO 8601, UTC"
    },
    "locked": {
      "$id": "#/properties/locked",
      "type": "boolean",
      "description": "",
      "examples": [
        "true",
        "false"
      ]
    }
  },
  "examples": [
    {
      "eventTimestamp": "2020-10-31T08:38:02.749Z",
      "locked": false
    }
  ]
}
